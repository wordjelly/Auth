<p id="notice"><%= notice %></p>

<h1>Listing Auth Shopping Products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_shopping_products.each do |auth_shopping_product| %>
      <tr>
        <td><%= link_to 'Show', product_path(auth_shopping_product) %></td>
        <td><%= link_to 'Edit', edit_product_path(auth_shopping_product) %></td>
        <td><%= link_to 'Destroy', product_path(auth_shopping_product), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
