<h1>Listing Tests</h1>

<table class="bordered">
  <thead>
    <tr>
      <th>Price</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cart_items.each do |test| %>
      <tr>
        <td>Rs.<%= test.price %></td>
        <td><%= test.name %></td>
        <td><%= test.description %></td>
        <td><%= link_to 'Show', shopping_cart_item_path({:id => test.id, :cart_item => {:public => true}}) %></td>
        <td><%= link_to 'Add to Cart',shopping_cart_items_path({:cart_item => test.attributes}), method: :post %></td>
        <td></td>
        <% if lookup_resource %>
        <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>