<h1>Your Tests Cart looks like this</h1>

<% cart_items.each do |cart_item| %>
  <div class="m-20-bottom m-20-top">
    <% Auth.configuration.cart_item_class.constantize.fields.keys.sort.each do |field| %>
      <% if field=~/name|price/ %>
        <div><%= cart_item.send("#{field}") or "" %></div>
      <% end %>
    <% end %>
  </div>
  <div class="divider"></div>
<% end %>