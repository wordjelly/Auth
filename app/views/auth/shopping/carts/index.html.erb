<p id="notice"><%= notice %></p>

<h1>Listing Auth Shopping Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_shopping_carts.each do |auth_shopping_cart| %>
      <tr>
        <td><%= link_to 'Show', cart_path(auth_shopping_cart) %></td>
        <td><%= link_to 'Edit', edit_cart_path(auth_shopping_cart) %></td>
        <td><%= link_to 'Destroy', cart_path(auth_shopping_cart), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cart', new_auth_shopping_cart_path %>
