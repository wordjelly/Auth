<p id="notice"><%= notice %></p>

<h1>Listing Auth Assemblies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_assemblies.each do |auth_assembly| %>
      <tr>
        <td><%= link_to 'Show', auth_assembly %></td>
        <td><%= link_to 'Edit', edit_auth_assembly_path(auth_assembly) %></td>
        <td><%= link_to 'Destroy', auth_assembly, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assembly', new_auth_assembly_path %>
